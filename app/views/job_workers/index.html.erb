<h1>Listing job_workers</h1>

<table>
  <tr>
    <th>Job</th>
    <th>Worker</th>
    <th>Iscreator</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @job_workers.each do |job_worker| %>
  <tr>
    <td><%= job_worker.job_id %></td>
    <td><%= job_worker.worker_id %></td>
    <td><%= job_worker.iscreator %></td>
    <td><%= link_to 'Show', job_worker %></td>
    <td><%= link_to 'Edit', edit_job_worker_path(job_worker) %></td>
    <td><%= link_to 'Destroy', job_worker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

